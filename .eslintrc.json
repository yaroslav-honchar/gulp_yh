{
  "parser": "@babel/eslint-parser",
  "extends": "airbnb",
  "overrides": [
    {
      "files": [
        "src/**/*.js"
      ],
      "extends": [
        "eslint:recommended"
      ],
      "rules": {
        "no-unused-vars": "warn",
        "indent": [
          "error",
          2
        ],
        "semi": [
          "error",
          "never"
        ],
        "quotes": [
          "error",
          "double"
        ]
      }
    }
  ]
}